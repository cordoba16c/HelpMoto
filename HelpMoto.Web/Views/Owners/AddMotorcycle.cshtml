@model HelpMoto.Web.Models.MotorcycleViewModel
@{
    ViewData["Title"] = "AddMotorcycle";
}
<h2>AddMotorcycle</h2>

<h4>Motorcycle</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="AddMotorcycle" enctype="multipart/form-data">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="OwnerId" />

            <partial name="_Motorcycle" />

            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
                <a asp-action="Details" asp-route-id="@Model.OwnerId" class="btn btn-success">Back to Owner</a>
            </div>
        </form>
    </div>
</div>